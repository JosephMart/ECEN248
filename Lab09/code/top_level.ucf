#Switches
NET "SWs[0]" LOC = "L13" | IOSTANDARD = LVTTL; #SW0
NET "SWs[1]" LOC = "L14" | IOSTANDARD = LVTTL; #SW1

#Push-buttons
NET "NORTH" LOC = "V4" | IOSTANDARD = LVttL | PULLDOWN; #North
NET "SOUTH" LOC = "K17" | IOSTANDARD = LVTTL | PULLDOWN; #South

# LEDs
NET "LEDs[0]" LOC = "F12" | IOSTANDARD = LVTTL; #LD0
NET "LEDs[1]" LOC = "E12" | IOSTANDARD = LVTTL; #LD1
NET "LEDs[2]" LOC = "E11" | IOSTANDARD = LVTTL; #LD2
NET "LEDs[3]" LOC = "F11" | IOSTANDARD = LVTTL; #LD3
NET "LEDs[4]" LOC = "C11" | IOSTANDARD = LVTTL; #LD0

NET "FastClk" LOC = "C9" | IOSTANDARD = LVTTL;

# Define clock period for 50 MHz oscillator
NET "FastClk" PERIOD = 20.0ns HIGH 40%;
